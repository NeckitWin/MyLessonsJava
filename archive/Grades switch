//        Напиши оценку, которую получил:
        int grade = 0;
        switch (grade){
            case 1:
                System.out.println("Очень плохо!");
                break;
            case 2:
                System.out.println("Плохо");
                break;
            case 3:
                System.out.println("Неплохо");
                break;
            case 4:
                System.out.println("Хорошо");
                break;
            case 5:
                System.out.println("Очень хорошо!");
                break;
            default:
                System.out.println("Вы не получили сегодня оценку");
                break;
        }